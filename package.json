{
  "name": "readyplayerone",
  "main": "expo-router/entry",
  "version": "1.1.0",
  "scripts": {
    "start": "expo start",
    "start:phone": "expo start --tunnel",
    "start:lan": "expo start --lan",
    "start:usb": "expo start --localhost",
    "start:clear": "expo start --clear",
    "start:phone:clear": "expo start --tunnel --clear",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "android:device": "expo run:android --device",
    "ios": "expo run:ios",
    "ios:device": "expo run:ios --device",
    "web": "expo start --web",
    "lint": "expo lint",
    "clear": "expo start --clear",
    "clear:all": "rm -rf node_modules/.cache .expo && npm run clear",
    "logs:ios": "xcrun simctl spawn booted log stream --predicate 'processImagePath contains \"PickMeGames\"' --level debug",
    "logs:device": "xcrun devicectl device process monitor --device $(xcrun devicectl list devices | grep -i iphone | head -1 | awk '{print $1}') 2>/dev/null || echo 'Use Console.app or Xcode → Window → Devices to view logs'"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/slider": "4.5.6",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@tamagui/button": "^1.126.14",
    "@tamagui/config": "^1.126.14",
    "@tamagui/core": "^1.126.14",
    "@tamagui/image": "^1.126.14",
    "@tamagui/input": "^1.126.14",
    "@tamagui/lucide-icons": "^1.126.14",
    "@tamagui/stacks": "^1.126.14",
    "@tamagui/text": "^1.126.14",
    "expo": "53.0.13",
    "expo-application": "~6.1.4",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.6",
    "expo-dev-client": "~5.2.1",
    "expo-device": "~7.1.4",
    "expo-file-system": "~18.1.10",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.3.0",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.5",
    "expo-localization": "~16.1.5",
    "expo-router": "~5.1.1",
    "expo-splash-screen": "~0.30.9",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.9",
    "expo-web-browser": "~14.2.0",
    "posthog-react-native": "^4.0.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.4",
    "react-native-confetti-cannon": "^1.5.2",
    "react-native-device-info": "^14.0.4",
    "react-native-draggable-flatlist": "^4.0.3",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@react-native-community/cli": "latest",
    "@types/react": "~19.0.10",
    "dotenv": "^16.4.5",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "typescript": "~5.8.3"
  },
  "private": true
